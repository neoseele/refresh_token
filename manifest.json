{
  "name": "refresh_token",
  "description": "Nil's first chrome extension",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "tabs",
    "downloads",
    "<all_urls>"
  ],
  "browser_action": {
    "default_name": "Refresh Token"
  },
  "background":{
    "scripts":[
      "background.js"
    ]
  },
  "manifest_version": 2
}
